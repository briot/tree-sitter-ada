{
  "name": "tree-sitter-ada",
  "version": "0.1.0",
  "description": "Ada grammar for tree-sitter",
  "repository": "https://github.com/briot/tree-sitter-ada",
  "license": "MIT",
  "author": {
    "name": "Emmanuel Briot"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "ada"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "generate": "tree-sitter generate --abi=14",
    "highlight": "tree-sitter highlight",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "tree-sitter generate && time tree-sitter test && node --test bindings/node/*_test.js",
    "testquick": "time tree-sitter test",
    "update-test": "tree-sitter test --update"
  }
}
